uniform vec3 vignette_color : source_color = vec3(0.0,0.0,0.0);

void fragment() {
    float dist = distance(UV, vec2(0.5, 0.5));
    float vignette = smoothstep(0.8, 1.0 - intensity, dist);
	COLOR = vec4(vignette_color,1.0-vignette);
}
